package com.demo;

public class HelloWorld {
	public static void main(String[] args){
		int[] arrNum = {2,44,56,12,345};
		for (int i : arrNum) {
		    System.out.println(i);
		}
		int n = sumOfNumbers();
		System.out.println(n);
//		Car myCar = new Car();
//		System.out.println(myCar.speed);
	}
	public static int sumOfNumbers() {
	    int sum = 0;
	    for(int i = 0; i <= 100; i++) {
	        sum += i;
	    }
	    return sum;
	}
}
